{
	"bezeichnungLang": "svelte-kit-tiny-project-starter",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:staging": "vite dev --mode staging",
		"dev:docker": "vite dev --mode docker",
		"build": "vite build --mode production",
		"build:dev": "vite build --mode development",
		"build:staging": "vite build --mode staging",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore\"",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --max-warnings 0 --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest run --segfault-retry=3 --dangerouslyIgnoreUnhandledErrors",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@lottiefiles/svelte-lottie-player": "^0.3.0",
		"@svgdotjs/svg.js": "^3.2.0",
		"@types/bootstrap": "^5.2.6",
		"bootstrap": "^5.3.0",
		"honeycomb-grid": "^4.1.4",
		"popper.js": "^1.16.1",
		"vite": "^4.4.6"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@tsconfig/svelte": "^5.0.0",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-cypress": "^2.13.3",
		"eslint-plugin-svelte": "^2.32.4",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"purgecss-from-svelte": "^2.0.2",
		"sass": "^1.64.0",
		"source-map-support": "^0.5.21",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.6",
		"svelte-material-icons": "^3.0.5",
		"svelte-preprocess": "^5.0.4",
		"svooltip": "^0.7.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"uuid": "^9.0.0"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env"
			]
		]
	},
	"browserslist": [
		"since 2017-06"
	]
}
